package com.jeedanandroidapps.quiz;

public class TrueFalse {
	private int mQuestion;
	private boolean mTrueQuestion;
	private boolean mCheated;
	
	public TrueFalse(int question, boolean trueQuestion, boolean hasCheated){
		mQuestion = question;
		mTrueQuestion = trueQuestion;
		mCheated = hasCheated;
	}
	
	public int getQuestion(){
		return mQuestion;
	}
	
	public void setQuestion(int value){
		mQuestion = value;
	}
	
	public boolean getTrueQuestion(){
		return mTrueQuestion;
	}
	
	public void setTrueQuestion(boolean value){
		mTrueQuestion = value;
	}	
	public boolean getCheated(){
		return mTrueQuestion;
	}
	
	public void setTrueQuestion(boolean value){
		mTrueQuestion = value;
	}
}
