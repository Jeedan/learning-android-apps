package com.jeedan.android.tvshowtracker;

import java.util.ArrayList;

public class TVSeason {
	
	private String mTitle;
	private int mSeasonCount;
	private ArrayList<TVEpisode> mEpisodes;
	
	public TVSeason(String title, int seasonCount,ArrayList<TVEpisode> episodes){
		mEpisodes = episodes;
		mTitle = title;
		mSeasonCount = seasonCount;
	}
}
